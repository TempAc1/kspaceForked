<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayDefaultChildHomeworkFragAdm"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeworkActAdm.HomeworkDefaultFragAdm">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerVHomeworkDefaultFragAdm"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp">

    </androidx.recyclerview.widget.RecyclerView>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingBtnHomeworkDefaultFragAdm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:clickable="true"
        android:elevation="5dp"
        android:src="@drawable/contract" />
</FrameLayout>