<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragAdm.HomeFragAdm">

    <!-- TODO: Update blank fragment layout -->
    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/RecyclerVHomeFragAdm"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </androidx.recyclerview.widget.RecyclerView>

    </FrameLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionBtnHomeFragAdm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:clickable="true"
        android:elevation="4dp"
        android:soundEffectsEnabled="true"
        android:src="@drawable/ic_baseline_add_circle_24"
        app:fabSize="auto"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:pressedTranslationZ="12dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/createPostFloatingActBtnHomeFragAdm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:clickable="true"
        android:elevation="4dp"
        android:src="@drawable/ic_baseline_edit_24"
        android:visibility="gone"
        app:fabSize="auto"
        app:layout_constraintBottom_toTopOf="@+id/floatingActionBtnHomeFragAdm"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionBtnHomeFragAdm"
        app:pressedTranslationZ="12dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/createEventFloatingActBtnHomeFragAdm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:clickable="true"
        android:src="@drawable/ic_baseline_event_24"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/createPostFloatingActBtnHomeFragAdm"
        app:layout_constraintEnd_toEndOf="@+id/createPostFloatingActBtnHomeFragAdm" />

</androidx.constraintlayout.widget.ConstraintLayout>