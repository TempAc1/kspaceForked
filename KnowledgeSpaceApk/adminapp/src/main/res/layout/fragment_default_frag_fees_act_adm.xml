<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayDefaultFragFeesActAdm"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="10dp"
    android:paddingTop="20dp"
    android:paddingRight="10dp"
    tools:context=".FeesActAdm.DefaultFragFeesActAdm">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_marginBottom="20dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/viewFeesBtnDefaultFFeesActAdm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="10dp"
                android:layout_weight="1"
                android:minHeight="50dp"
                android:text="View Fees" />

            <Button
                android:id="@+id/downloadFeesBtnDefaultFFeesActAdm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_weight="1"
                android:minHeight="50dp"
                android:text="Download Fees" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Enter Role"
            android:textSize="20sp" />

        <EditText
            android:id="@+id/roleEditTvFeesActMainScAdm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_baseline_person_24"
            android:ems="10"
            android:hint="Enter Role"
            android:inputType="textPersonName"
            android:maxLength="1"
            android:minHeight="50dp"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Enter Password"
            android:textSize="20sp" />

        <EditText
            android:id="@+id/rolePassEditTvFeesActMainScAdm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawableEnd="@drawable/ic_baseline_lock_24"
            android:ems="10"
            android:hint="Enter Role Assigned Password"
            android:inputType="text|number"
            android:maxLines="1"
            android:minHeight="50dp" />

        <View
            android:id="@+id/divider14"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="?android:attr/listDivider" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Select Fee Status"
                android:textSize="20sp" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <com.google.android.material.chip.ChipGroup
                    android:id="@+id/chipGrpDefaultFragFeesActAdm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:selectionRequired="true"
                    app:singleSelection="true">

                    <com.google.android.material.chip.Chip
                        android:id="@+id/feesPaidChipBtnChipGrpDefaultFragFeesActAdm"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:checkable="true"
                        android:clickable="true"
                        android:focusable="true"
                        android:minHeight="50dp"
                        android:padding="10dp"
                        android:text="Paid"
                        android:textAlignment="center" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/feesNotPaidChipBtnChipGrpDefaultFragFeesActAdm"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:checkable="true"
                        android:clickable="true"
                        android:focusable="true"
                        android:minHeight="50dp"
                        android:padding="10dp"
                        android:text="Not Paid"
                        android:textAlignment="center" />

                    <com.google.android.material.chip.Chip
                        android:id="@+id/allFeesChipBtnChipGrpDefaultFragFeesActAdm"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:checkable="true"
                        android:clickable="true"
                        android:focusable="true"
                        android:minHeight="50dp"
                        android:padding="10dp"
                        android:text="All "
                        android:textAlignment="center" />

                </com.google.android.material.chip.ChipGroup>
            </LinearLayout>

            <TextView
                android:id="@+id/textView15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Select Department / \Class"
                android:textSize="20sp" />

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/textInputLayDefaultFeesFragAdm"
                style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteTextViewDefaultFFeesAdm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:hint="Default"
                    android:inputType="none" />
            </com.google.android.material.textfield.TextInputLayout>

        </LinearLayout>

    </LinearLayout>

</FrameLayout>